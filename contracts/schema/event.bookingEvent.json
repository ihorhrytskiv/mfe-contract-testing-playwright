{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "bookingEvent",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "special",
        "submit",
        "error"
      ]
    },
    "payload": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "price": {
          "type": "number"
        }
      },
      "required": [
        "id"
      ],
      "additionalProperties": true
    }
  },
  "required": [
    "type",
    "payload"
  ],
  "additionalProperties": false
}